{
  "name": "kitchen-sink-app",
  "version": "0.0.1",
  "description": "A full stack project built with Python, Flask, and Create-React-App.",
  "main": "./server/main.py",
  "scripts": {
    "start:dev:server": "pipenv run flask run --port=5001 --no-debugger",
    "start:dev:client": "cd client && yarn start",
    "start:dev": "concurrently \"yarn run start:dev:server\" \"yarn run start:dev:client\"",
    "start:prod": "pipenv run flask run --port=5000 --no-debugger",
    "install": "concurrently \"cd client && yarn\" \"pipenv install\"",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "build": "cd client && npm run build && mv build ../server/src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ocskier/kitchen-sink.git"
  },
  "keywords": [
    "Python",
    "React",
    "Flask"
  ],
  "author": "Jon Jackson <ocskier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocskier/kitchen-sink/issues"
  },
  "homepage": "https://github.com/ocskier/kitchen-sink#README",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "if-env": "^1.0.4"
  },
  "dependencies": {}
}
